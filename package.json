{
  "name": "sudo-ui",
  "version": "0.2.0",
  "files": [
    "dist/lib/*",
    "src/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build-docs && npm run bundle-js && npm run bundle-scss",
    "build-docs": "vue-cli-service build",
    "bundle-js": "vue-cli-service build --dest ./dist/lib --target lib --name sudo-ui ./src/index.js",
    "bundle-scss": "mkdir ./dist/lib/scss && cp -a ./src/scss/. ./dist/lib/scss/.",
    "copy-package": "cp ./package.json ./dist/lib/package.json",
    "now-build": "npm run build"
  },
  "dependencies": {
    "vue": "^2.6.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "babel-plugin-module-resolver": "^3.1.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  }
}
